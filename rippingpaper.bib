issn = {1936-086X},
journal = {ACS nano},
keywords = {10 ϫ 15 films,conducting,deposition,electrode,flexible,have been,mer 9 and graphene,nanowires,studied,the most common material,transparent,used,way to large area,while poly-},
mendeley-groups = {Graphene ripping / membrane self-healing},
month = jul,
number = {7},
pages = {1767--74},
pmid = {19552383},
title = {{Silver Nanowire Networks as Flexible, Transparent, Conducting Films: Extremely High DC to Optical Conductivity Ratios.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19552383},
volume = {3},
year = {2009}
}

