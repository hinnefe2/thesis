}

@article{Lee2011,
abstract = {With the emergence of human interface technology, the development of new applications based on stretchable electronics such as conformal biosensors and rollable displays are required. However, the difficulty in developing semiconducting materials with high stretchability required for such applications has restricted the range of applications of stretchable electronics. Here, we present stretchable, printable, and transparent transistors composed of monolithically patterned graphene films. This material offers excellent mechanical, electrical, and optical properties, capable of use as semiconducting channels as well as the source/drain electrodes. Such monolithic graphene transistors show hole and electron mobilities of 1188 ± 136 and 422 ± 52 cm(2)/(V s), respectively, with stable operation at stretching up to 5\% even after 1000 or more cycles.},
