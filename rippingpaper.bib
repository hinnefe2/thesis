journal = {Small},
keywords = {Graphite,Graphite: chemistry,Molecular Dynamics Simulation,Nanostructures,Nanostructures: chemistry,Nanotechnology,Nanotechnology: methods,Surface Properties},
mendeley-groups = {Graphene ripping / membrane self-healing},
month = may,
number = {10},
pages = {1108--16},
pmid = {20449852},
title = {{Tearing graphene sheets from adhesive substrates produces tapered nanoribbons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20449852},
