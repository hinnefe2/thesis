 issue = {nan},
 journal = {Physical Review Letters},
 keyword = {nan},
 language = {nan},
 link = {http://prl.aps.org/abstract/PRL/v109/i6/e066802},
 mendeley-groups = {Graphene strain stuff,Graphene strain stuff/Theory},
